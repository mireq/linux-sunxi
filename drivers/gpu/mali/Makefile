# Configuration for A10

ifdef CONFIG_ARCH_SUN7I
CONFIG:=ca7-virtex820-m400-2
else
CONFIG:=ca8-virtex820-m400-1
endif

USING_MMU:=1
USING_UMP:=0
USING_PMM:=1
BUILD:=release
MALI_PLATFORM:=sunxi
ifneq ($(CONFIG_TRACEPOINTS),y)
USING_PROFILING:=0
export USING_PROFILING
endif
export CONFIG USING_MMU USING_UMP USING_PMM BUILD MALI_PLATFORM

obj-$(CONFIG_MALI) += ump/ mali/
